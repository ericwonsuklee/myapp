<div class="container">
  <div class="page-header" action="map/<%= @trip.id %>">
    <h2>Map for <%= @trip.trip_name %></h2>
  </div>
    <p>Location: <%= @trip.location %></p>

  <iframe
    width="600"
    height="450"
    frameborder="0" style="border:0"
    src="https://www.google.com/maps/embed/v1/place?key=AIzaSyArRtDF3JAJot9lfGLOw4bR-DNPkg88Jys
      &q=<%= @trip.location %>">
  </iframe>


</div>







